{
  "name": "setlist-sherlock-apple-music-token-server",
  "version": "2.0.0",
  "type": "module",
  "description": "Lambda function that creates restricted developer tokens for Android Apple Music users",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "scripts": {
    "build": "parcel build --target function"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylmye/setlist-sherlock-apple-music-token-server.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "apple-music",
    "apple"
  ],
  "author": "Dylan Myers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dylmye/setlist-sherlock-apple-music-token-server/issues"
  },
  "homepage": "https://github.com/dylmye/setlist-sherlock-apple-music-token-server#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/scaleway-functions": "^1.0.3",
    "parcel": "^2.16.3",
    "typescript": "5.9.3"
  },
  "targets": {
    "function": {
      "context": "node",
      "outputFormat": "esmodule",
      "scopeHoist": false,
      "optimize": true,
      "includeNodeModules": {
        "jose": true
      }
    }
  }
}
